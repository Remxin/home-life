datasource db {
  // could be postgresql or mysql
  provider = "postgres"
  url      = "postgresql://prisma:prisma@localhost:5432/home-life"
}
 
generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}


model User {
    id          String @id() @default(uuid())
    name        String
    email       String @unique()
    password    String
}